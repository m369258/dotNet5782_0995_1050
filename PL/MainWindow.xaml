<Window x:Class="PL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:bb="http://schemas.fontawesome.io/icons/"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="bntManager" Content="Manager" Click="ShowProductsButton_Click" HorizontalAlignment="Left" Margin="483,72,0,319" RenderTransformOrigin="-1.054,1.652"/>
        <Button x:Name="btnCategory" Content="Catalog" HorizontalAlignment="Center" VerticalAlignment="Center" Click="category_Click"/>
        <TextBox x:Name="txtOrderId" HorizontalAlignment="Center" Margin="0,288,0,0" TextWrapping="Wrap" Text="order id" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.422,-1.273"/>
        <Button x:Name="btnTrack" Content="Track" HorizontalAlignment="Center" Margin="0,315,0,0" VerticalAlignment="Top" Click="btnTrack_Click"/>
        <Button x:Name="btnCustomer" Content="Customer" HorizontalAlignment="Left" Margin="195,72,0,0" VerticalAlignment="Top" Click="btnCustomer_Click"/>

    </Grid>
</Window>
